= client_form.input :first_name, disabled: disabled
= client_form.input :middle_name, disabled: disabled
= client_form.input :last_name, disabled: disabled
= client_form.input :date_of_birth, as: :date_picker, disabled: disabled
= client_form.input :ssn, label: "SSN", input_html: {maxlength: 9}, disabled: disabled
- if can_edit_all_clients?
  .jAgency
    = client_form.association :agency, input_html: {class: :select2}, disabled: disabled
= client_form.input :contact_id, collection: @contacts, as: :grouped_select, group_method: :last, selected: @contact_id, include_blank: 'None', label: "Default #{_('Shelter Agency')} Contact", input_html: {class: 'select2 jContact' }, disabled: disabled
- if can_add_cohorts_to_deidentified_clients? && Warehouse::Base.enabled?
  = client_form.input :active_cohort_ids, collection: Warehouse::Cohort.active, input_html: { multiple: true, class: 'select2' }, label: "Cohorts", disabled: disabled
= client_form.input :limited_release_on_file, label: 'Limited Coordinated Access System Release on file', disabled: disabled
= client_form.input :full_release_on_file, label: 'Full Housing Access Network Release on file', disabled: disabled
= client_form.input :active_client, disabled: disabled
= client_form.input :eligible_for_matching, disabled: disabled
