.c-card
  .c-card__header
    %h2 Average Successful Match Length
  .c-card__content.p-0
    %table.table.mb-0.table-striped
      %thead
        %tr
          %th Step
          %th Average Days
      %tbody
        - @report.average_days_initiation_to_step_completion.each do |(_, match_step), count|
          %tr
            %td Match Initiation to #{match_step}
            %td= count.round
